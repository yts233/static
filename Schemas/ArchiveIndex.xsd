<?xml version="1.0" encoding="utf-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="https://static.yts233.tk/Schemas/ArchiveIndex.xsd">
    <xs:element name="archives" type="archivesType">
        <xs:annotation>
            <xs:documentation>文档索引</xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="archivesType">
        <xs:sequence>
            <xs:element name="archive" minOccurs="0" maxOccurs="unbounded" type="archiveType">
                <xs:annotation>
                    <xs:documentation>一个文档</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="archive" type="archiveType">
        <xs:annotation>
            <xs:documentation>一个文档</xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="translationsType">
        <xs:sequence>
            <xs:element name="translation" minOccurs="0" maxOccurs="unbounded" type="translateType">
                <xs:annotation>
                    <xs:documentation>翻译</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="statusType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="editing">
                <xs:annotation>
                    <xs:documentation>编辑中</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="archived">
                <xs:annotation>
                    <xs:documentation>已存档</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="outdated">
                <xs:annotation>
                    <xs:documentation>过时</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="archiveType">
        <xs:sequence>
            <xs:element name="id" type="xs:int" default="-1">
                <xs:annotation>
                    <xs:documentation>文档的标识符</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="time" type="xs:dateTime" default="2021-01-01 0:00:00 +8">
                <xs:annotation>
                    <xs:documentation>文档的时间</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="status" type="statusType" default="editing">
                <xs:annotation>
                    <xs:documentation>文档状态</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="prime" type="translateType">
                <xs:annotation>
                    <xs:documentation>主语言</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="translations" type="translationsType">
                <xs:annotation>
                    <xs:documentation>翻译</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="translateType">
        <xs:sequence>
            <xs:element name="title" type="xs:string">
                <xs:annotation>
                    <xs:documentation>文档的标题</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="description" type="xs:string">
                <xs:annotation>
                    <xs:documentation>文档的描述</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="files" type="filesType">
                <xs:annotation>
                    <xs:documentation>文档的文件</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="default" type="defaultType">
                <xs:annotation>
                    <xs:documentation>默认文档</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="lang" type="languageType" use="required" default="zh-CN">
            <xs:annotation>
                <xs:documentation>语言</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
